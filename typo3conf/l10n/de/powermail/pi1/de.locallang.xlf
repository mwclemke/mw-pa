        <source>Please enter a valid number in this field</source>
      <target state="translated">Bitte eine Nummer eintragen</target></trans-unit>
      <trans-unit id="JSvalidation_label_validate_digits" xml:space="preserve" approved="yes">
        <source>Please use numbers only in this field. please avoid spaces or other characters such as dots or commas</source>
      <target state="translated">Bitte eine Nummer ohne Komma oder Punkt eintragen</target></trans-unit>
      <trans-unit id="JSvalidation_label_validate_alpha" xml:space="preserve" approved="yes">
        <source>Please use letters only (a-z) in this field</source>
      <target state="translated">Bitte nur Buchstaben (a-z) eingeben</target></trans-unit>
      <trans-unit id="JSvalidation_label_validate_alphanum" xml:space="preserve" approved="yes">
        <source>Please use only letters (a-z) or numbers (0-9) only in this field. No spaces or other characters are allowed</source>
      <target state="translated">Bitte nur Buchstaben (a-z) oder Zahlen (0-9) eingeben. Leer- oder andere Zeichen sind nicht erlaubt</target></trans-unit>
      <trans-unit id="JSvalidation_label_validate_date" xml:space="preserve" approved="yes">
        <source>Please enter a valid date</source>
      <target state="translated">Bitte ein gültiges Datum eingeben</target></trans-unit>
