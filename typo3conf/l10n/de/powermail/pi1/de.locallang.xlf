        <source>Please enter a valid email address (test@test.com)</source>
      <target state="translated">Bitte eine gültige E-Mail-Adresse eingeben (test@test.com)</target></trans-unit>
      <trans-unit id="JSvalidation_label_validate_url" xml:space="preserve" approved="yes">
        <source>Please enter a valid URL (http://www.test.com)</source>
      <target state="translated">Bitte eine gültige URL eingeben (z.B. http://www.test.de)</target></trans-unit>
      <trans-unit id="JSvalidation_label_validate_date_au" xml:space="preserve" approved="yes">
        <source>Please use this date format: dd/mm/yyyy. For example 17/03/2006 for the 17th of March, 2006</source>
      <target state="translated">Bitte ein Datum im Format dd/mm/yyyy eingeben</target></trans-unit>
      <trans-unit id="JSvalidation_label_validate_currency_dollar" xml:space="preserve" approved="yes">
        <source>Please enter a valid $ amount. For example $100.00</source>
      <target state="translated">Bitte einen gültigen Betrag eingeben (z.B. $100.00)</target></trans-unit>
      <trans-unit id="JSvalidation_label_validate_selection" xml:space="preserve" approved="yes">
        <source>Please make a selection</source>
      <target state="translated">Bitte treffen Sie eine Auswahl</target></trans-unit>
      <trans-unit id="JSvalidation_label_validate_one_required" xml:space="preserve" approved="yes">
        <source>Please select one of the below options</source>
      <target state="translated">Bitte treffen Sie mindestens eine Auswahl</target></trans-unit>
      <trans-unit id="JSvalidation_label_validate_max" xml:space="preserve" approved="yes">
        <source>Please enter a value greater than $1</source>
      <target state="translated">Bitte einen Wert grösser als $1 eingeben</target></trans-unit>
      <trans-unit id="JSvalidation_label_validate_min" xml:space="preserve" approved="yes">
        <source>Please enter a value smaller than $1</source>
      <target state="translated">Bitte einen Wert kleiner als $1 eingeben</target></trans-unit>
