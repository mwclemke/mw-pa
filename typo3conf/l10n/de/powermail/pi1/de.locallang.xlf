      <target state="translated">Diese Datei ist zu groß:</target></trans-unit>
      <trans-unit id="locallangmarker_error_file_extension" xml:space="preserve" approved="yes">
        <source>This file extension is not allowed:</source>
      <target state="translated">Diese Dateiendung ist nicht erlaubt:</target></trans-unit>
      <trans-unit id="locallangmarker_error_file_mimetype" xml:space="preserve" approved="yes">
        <source>Uploaded file don't fits mime check:</source>
      <target state="translated">Die hochgeladene Datei hat nicht den richtigen mime-Typ.</target></trans-unit>
      <trans-unit id="error_check_recipient" xml:space="preserve" approved="yes">
        <source>Please fill in this backend field: Email address of receiver</source>
      <target state="translated">Empfängerfeld im Backend nicht ausgefüllt!</target></trans-unit>
      <trans-unit id="error_check_noTS" xml:space="preserve" approved="yes">
        <source>Typoscript for powermail missing!</source>
      <target state="translated">Powermail - TypoScript nicht vorhanden!</target></trans-unit>
      <trans-unit id="error_captcha_empty" xml:space="preserve" approved="yes">
        <source>Captcha Code empty</source>
      <target state="translated">Bitte den angezeigten Captcha-Code eintragen</target></trans-unit>
      <trans-unit id="error_captcha_wrong" xml:space="preserve" approved="yes">
        <source>Captcha Code wrong</source>
      <target state="translated">Der eingetragene Code war leider falsch</target></trans-unit>
      <trans-unit id="error_backend_noip" xml:space="preserve" approved="yes">
        <source>IP logging disabled</source>
      <target state="translated">IP-Speicherung deaktiviert</target></trans-unit>
      <trans-unit id="error_validemail" xml:space="preserve" approved="yes">
        <source>Email address is not valid</source>
      <target state="translated">Die eingegebenen E-Mail-Adresse enthält Fehler</target></trans-unit>
      <trans-unit id="error_nomx" xml:space="preserve" approved="yes">
        <source>Email address doesn't exist</source>
      <target state="translated">Die eingegebene E-Mail-Adresse existiert nicht</target></trans-unit>
      <trans-unit id="error_unique_field" xml:space="preserve" approved="yes">
        <source>%s was already used</source>
      <target state="translated">%s wurde bereits eingetragen</target></trans-unit>
      <trans-unit id="error_unique_ip" xml:space="preserve" approved="yes">
        <source>IP address %s already made an entry</source>
      <target state="translated">Ein Besucher mit der IP-Adresse %s hat bereits einen Eintrag hinterlassen</target></trans-unit>
      <trans-unit id="error_expression_validation" xml:space="preserve" approved="yes">
        <source>Validation failed</source>
      <target state="translated">Überprüfung fehlgeschlagen</target></trans-unit>
